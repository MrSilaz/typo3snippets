	"t3FluidCycle": {
		"prefix": "t3FluidCycle",
		"body": [
			"<f:cycle values=\"{0: '$1', 1: '$2', 2: '$3'}\" as=\"$4\">",
      		"	{$4}",
    		"</f:cycle>"
		],
		"description": [
			"TYPO3 Fluid | <f:cycle values=\"{0: 'green', 1: 'red', 2: 'blue'}\" as=\"color\">
      			{color}
    		</f:cycle>"
		]
	},

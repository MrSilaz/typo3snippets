		"body": [
			"{$1 -> f:format.printf(arguments: {1: '$2'})}"
		],
		"description": "TYPO3 Fluid inline | {someText -> f:format.printf(arguments: {1: 'TYPO3'})}"
	},
	"t3FluidFormatRaw": {

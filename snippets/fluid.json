		],
		"description": "TYPO3 Fluid inline | {f:cycle(values: {foo: 'bar'}, as: 'foo')}"
	},
	"t3FluidDebug": {
		"prefix": "t3FluidDebug",
		"body": [
			"<f:debug title=\"$1\" plainText=\"1\" inline=\"0\" >",
